{
  "name": "rnstarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "production": "yarn rnuc production.env",
    "staging": "yarn rnuc staging.env",
    "resetIos": "yarn staging && cd ios && pod deintegrate && rm -rf Podfile.lock && pod install && cd .. && yarn run ios",
    "resetAndroid": "yarn staging && cd android && ./gradlew clean && cd .. && yarn run android",
    "buildApkStaging": "yarn staging && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "buildApkProduction": "yarn production && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "pushAndroidStaging": "yarn staging && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-Android -d Staging",
    "pushIosStaging": "yarn staging && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-iOS -d Staging",
    "pushStaging": "yarn pushAndroidStaging && yarn pushIosStaging",
    "pushAndroidProduction": "yarn production && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-Android -d Production",
    "pushIosProduction": "yarn production && appcenter codepush release-react -a Mobilecoderz-ReactNative/RNStarter-iOS -d Production",
    "pushProduction": "yarn pushAndroidProduction && yarn pushIosProduction",
    "postinstall": "yarn staging && npx pod-install && npx patch-package"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^6.3.1",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@types/react-native-background-timer": "^2.0.0",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "axios": "^0.27.2",
    "deprecated-react-native-prop-types": "^2.3.0",
    "fs": "^0.0.1-security",
    "google-libphonenumber": "^3.2.30",
    "moment": "^2.29.1",
    "moti": "^0.17.1",
    "react": "18.0.0",
    "react-hook-form": "^7.34.0",
    "react-native": "0.69.3",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^3.2.3",
    "react-native-code-push": "^7.0.5",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-dropdownalert": "^4.5.1",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-image-crop-picker": "^0.38.0",
    "react-native-indicators": "^0.17.0",
    "react-native-internet-connection-alert": "^0.1.9",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^2.2.2",
    "react-native-material-menu": "^1.2.0",
    "react-native-mmkv-storage": "^0.8.0",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^13.0.0",
    "react-native-permissions": "^3.6.0",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.10.1",
    "react-native-snackbar": "^2.4.0",
    "react-native-ultimate-config": "^4.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.17.2",
    "react-query": "^3.39.2",
    "react-query-native-persist": "^0.1.0",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
